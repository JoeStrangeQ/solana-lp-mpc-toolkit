{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/health", "dest": "/api/index.ts" },
    { "src": "/fees(.*)", "dest": "/api/index.ts" },
    { "src": "/pools(.*)", "dest": "/api/index.ts" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
